        "gitDecoration.conflictingResourceForeground": "#FFCB6B",
        "gitDecoration.ignoredResourceForeground": "#546E7A",
        "editorError.foreground": "#F07178",
        "editorWarning.foreground": "#FFCB6B",
        "editorInfo.foreground": "#82AAFF"
    },
    "tokenColors": [
        {
            "name": "COMMENT",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "comment.block.documentation",
                "comment.line"
            ],
            "settings": {
                "foreground": "#546E7A",
                "fontStyle": "italic"
            }
        },
        {
            "name": "VARIABLE",
            "scope": [
                "variable",
                "variable.other",
                "variable.object"
            ],
            "settings": {
                "foreground": "#CFD8DC"
            }
        },
        {
            "name": "VARIABLE BUILTIN",
            "scope": [
                "variable.language",
                "variable.language.this",
                "variable.language.self",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#F07178"
            }
        },
        {
            "name": "VARIABLE PARAMETER",
            "scope": [
                "variable.parameter",
                "variable.parameter.function",
                "meta.function.parameters variable"
            ],
            "settings": {
                "foreground": "#FFCB6B"
            }
        },
        {
            "name": "VARIABLE MEMBER/PROPERTY",
            "scope": [
                "variable.other.property",
                "variable.other.member",
                "variable.other.object.property",
                "support.variable.property"
            ],
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "CONSTANT",
            "scope": [
                "constant",
                "constant.numeric",
                "constant.language",
                "constant.character",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#F78C6C"
            }
        },
        {
            "name": "STRING",
            "scope": [
                "string",
                "string.quoted",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#C3E88D"
            }
        },
        {
            "name": "STRING ESCAPE/REGEXP",
            "scope": [
                "string.regexp",
                "constant.character.escape",
                "string.template",
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "name": "TYPE",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#FFCB6B"
            }
        },
        {
            "name": "TYPE BUILTIN",
            "scope": [
                "support.type.builtin",
                "storage.type.builtin",
                "storage.type.primitive"
            ],
            "settings": {
                "foreground": "#C792EA",
                "fontStyle": "italic"
            }
        },
        {
            "name": "FUNCTION",
            "scope": [
                "entity.name.function",
                "support.function",
                "variable.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "FUNCTION MACRO",
            "scope": [
                "entity.name.function.macro",
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "name": "KEYWORD",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression",
                "storage.modifier",
                "storage.type.function"
            ],
            "settings": {
                "foreground": "#C792EA"
            }
        },
        {
            "name": "OPERATOR",
            "scope": [
                "keyword.operator",
                "punctuation.accessor",
                "punctuation.separator"
            ],
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "name": "PUNCTUATION",
            "scope": [
                "punctuation",
                "punctuation.definition.block",
                "punctuation.definition.parameters",
                "punctuation.section",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "name": "ATTRIBUTE",
            "scope": [
                "entity.other.attribute-name",
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "foreground": "#C792EA"
            }
        },
        {
            "name": "TAG",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#F07178"
            }
        },
        {
            "name": "NAMESPACE/MODULE",
            "scope": [
                "entity.name.namespace",
                "entity.name.module",
                "entity.name.package"
            ],
            "settings": {
                "foreground": "#FFCB6B",
                "fontStyle": "italic"
            }
        },
        {
            "name": "STORAGE TYPE",
            "scope": [
                "storage.type",
                "storage.type.class",
                "storage.type.function"
            ],
            "settings": {
                "foreground": "#C792EA"
            }
        },
        {
            "name": "SUPPORT CONSTANT",
            "scope": [
                "support.constant",
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#F78C6C"
            }
        },
        {
            "name": "JSON KEY",
            "scope": [
                "support.type.property-name.json",
                "source.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "MARKUP HEADING",
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#82AAFF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "MARKUP BOLD",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#F07178",
                "fontStyle": "bold"
            }
        },
        {
            "name": "MARKUP ITALIC",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#F07178",
                "fontStyle": "italic"
            }
        },
        {
            "name": "MARKUP LINK",
            "scope": [
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#89DDFF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "MARKUP CODE",
            "scope": [
                "markup.inline.raw",
                "markup.raw.block",
                "markup.fenced_code.block"
            ],
            "settings": {
                "foreground": "#C3E88D"
            }
        },
        {
            "name": "DIFF ADDED",
            "scope": [
                "markup.inserted",
                "markup.inserted.diff"
            ],
            "settings": {
                "foreground": "#C3E88D"
            }
        },
        {
            "name": "DIFF DELETED",
            "scope": [
                "markup.deleted",
                "markup.deleted.diff"
            ],
            "settings": {
                "foreground": "#F07178"
            }
        },
        {
            "name": "INVALID",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#F07178",
                "fontStyle": "bold"
            }
        }
    ]
}