{
    "name": "Flat-Theme Light",
    "type": "light",
    "semanticClass": "theme.flat-light",
    "semanticHighlighting": true,
    "colors": {
        "editor.background": "#F5F5F5",
        "editor.foreground": "#37474F",
        "editorLineNumber.foreground": "#90A4AE",
        "editorLineNumber.activeForeground": "#263238",
        "editor.selectionBackground": "#80CBC422",
        "editor.selectionHighlightBackground": "#80CBC410",
        "editor.selectionForeground": "#000000",
        "editorCursor.foreground": "#80CBC4",
        "editorWhitespace.foreground": "#CFD8DC",
        "editorIndentGuide.background1": "#E0E0E0",
        "editorIndentGuide.activeBackground1": "#80CBC4",
        "activityBar.background": "#F5F5F5",
        "activityBar.foreground": "#37474F",
        "activityBarBadge.background": "#80CBC4",
        "activityBarBadge.foreground": "#000000",
        "sideBar.background": "#F5F5F5",
        "sideBar.foreground": "#37474F",
        "sideBarSectionHeader.background": "#F5F5F5",
        "sideBarSectionHeader.foreground": "#37474F",
        "sideBarSectionHeader.border": "#F5F5F5",
        "list.hoverBackground": "#EEEEEE",
        "list.activeSelectionBackground": "#E0E0E099",
        "list.inactiveSelectionBackground": "#E0E0E099",
        "list.focusBackground": "#EEEEEE",
        "list.hoverForeground": "#37474F",
        "statusBar.background": "#F5F5F5",
        "statusBar.foreground": "#37474F",
        "statusBar.noFolderBackground": "#F5F5F5",
        "statusBar.noFolderForeground": "#37474F",
        "titleBar.activeBackground": "#F5F5F5",
        "titleBar.activeForeground": "#37474F",
        "button.background": "#80CBC4",
        "button.foreground": "#000000",
        "tab.activeBackground": "#F5F5F5",
        "tab.inactiveBackground": "#F5F5F5",
        "panel.background": "#F5F5F5",
        "panelTitle.activeBorder": "#80CBC4",
        "minimap.background": "#F5F5F5",
        "editorWidget.background": "#F5F5F5",
        "editorGutter.background": "#F5F5F5",
        "contrastActiveBorder": "#F5F5F5",
        "focusBorder": "#80CBC4",
        "editorGroup.border": "#F5F5F5",
        "sideBar.border": "#F5F5F5",
        "activityBar.border": "#F5F5F5",
        "statusBar.border": "#F5F5F5",
        "titleBar.border": "#F5F5F5",
        "tab.border": "#F5F5F5",
        "panel.border": "#F5F5F5",
        "scrollbar.shadow": "#00000000",
        "scrollbarSlider.background": "#BDBDBD66",
        "scrollbarSlider.hoverBackground": "#9E9E9E99",
        "scrollbarSlider.activeBackground": "#9E9E9Ecc",
        "input.background": "#FFFFFF",
        "input.foreground": "#37474F",
        "input.border": "#FFFFFF",
        "quickInput.background": "#FFFFFF",
        "quickInput.foreground": "#37474F",
        "quickInputList.focusBackground": "#80CBC4",
        "quickInputList.focusForeground": "#000000",
        "pickerGroup.border": "#F5F5F5",
        "editorSuggestWidget.background": "#FFFFFF",
        "editorSuggestWidget.border": "#FFFFFF",
        "settings.headerForeground": "#37474F",
        "settings.headerBorder": "#F5F5F5",
        "settings.settingsHeaderHoverForeground": "#37474F",
        "settings.rowHoverBackground": "#EEEEEE",
        "settings.focusedRowBackground": "#EEEEEE",
        "settings.focusedRowBorder": "#80CBC4",
        "settings.dropdownBackground": "#FFFFFF",
        "settings.dropdownForeground": "#37474F",
        "settings.dropdownBorder": "#FFFFFF",
        "settings.checkboxBackground": "#FFFFFF",
        "settings.checkboxForeground": "#37474F",
        "settings.checkboxBorder": "#FFFFFF",
        "settings.numberInputBackground": "#FFFFFF",
        "settings.numberInputForeground": "#37474F",
        "settings.numberInputBorder": "#FFFFFF",
        "settings.textInputBackground": "#FFFFFF",
        "settings.textInputForeground": "#37474F",
        "settings.textInputBorder": "#FFFFFF",
        "settings.modifiedItemIndicator": "#80CBC4",
        "menubar.selectionBackground": "#EEEEEE",
        "menubar.selectionForeground": "#37474F",
        "menubar.selectionBorder": "#F5F5F5",
        "menu.background": "#FFFFFF",
        "menu.foreground": "#37474F",
        "menu.border": "#FFFFFF",
        "menu.selectionBackground": "#EEEEEE",
        "menu.selectionForeground": "#37474F",
        "terminal.background": "#F5F5F5",
        "terminal.foreground": "#37474F",
        "terminal.ansiBlack": "#263238",
        "terminal.ansiRed": "#EF5350",
        "terminal.ansiGreen": "#7CB342",
        "terminal.ansiYellow": "#F9AE58",
        "terminal.ansiBlue": "#42A5F5",
        "terminal.ansiMagenta": "#AB47BC",
        "terminal.ansiCyan": "#26C6DA",
        "terminal.ansiWhite": "#ECEFF1",
        "terminal.ansiBrightBlack": "#546E7A",
        "terminal.ansiBrightRed": "#EF5350",
        "terminal.ansiBrightGreen": "#7CB342",
        "terminal.ansiBrightYellow": "#F9AE58",
        "terminal.ansiBrightBlue": "#42A5F5",
        "terminal.ansiBrightMagenta": "#AB47BC",
        "terminal.ansiBrightCyan": "#26C6DA",
        "terminal.ansiBrightWhite": "#FFFFFF",
        "gitDecoration.addedResourceForeground": "#7CB342",
        "gitDecoration.modifiedResourceForeground": "#F9AE58",
        "gitDecoration.deletedResourceForeground": "#EF5350",
        "gitDecoration.untrackedResourceForeground": "#7CB342",
        "gitDecoration.conflictingResourceForeground": "#F9AE58",
        "gitDecoration.ignoredResourceForeground": "#90A4AE",
        "editorError.foreground": "#EF5350",
        "editorWarning.foreground": "#F9AE58",
        "editorInfo.foreground": "#42A5F5"
    },
    "tokenColors": [
        {
            "name": "COMMENT",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "comment.block.documentation",
                "comment.line"
            ],
            "settings": {
                "foreground": "#90A4AE",
                "fontStyle": "italic"
            }
        },
        {
            "name": "VARIABLE",
            "scope": [
                "variable",
                "variable.other",
                "variable.object"
            ],
            "settings": {
                "foreground": "#37474F"
            }
        },
        {
            "name": "VARIABLE BUILTIN",
            "scope": [
                "variable.language",
                "variable.language.this",
                "variable.language.self",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#EF5350"
            }
        },
        {
            "name": "VARIABLE PARAMETER",
            "scope": [
                "variable.parameter",
                "variable.parameter.function",
                "meta.function.parameters variable"
            ],
            "settings": {
                "foreground": "#F9AE58"
            }
        },
        {
            "name": "VARIABLE MEMBER/PROPERTY",
            "scope": [
                "variable.other.property",
                "variable.other.member",
                "variable.other.object.property",
                "support.variable.property"
            ],
            "settings": {
                "foreground": "#42A5F5"
            }
        },
        {
            "name": "CONSTANT",
            "scope": [
                "constant",
                "constant.numeric",
                "constant.language",
                "constant.character",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#FF7043"
            }
        },
        {
            "name": "STRING",
            "scope": [
                "string",
                "string.quoted",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#7CB342"
            }
        },
        {
            "name": "STRING ESCAPE/REGEXP",
            "scope": [
                "string.regexp",
                "constant.character.escape",
                "string.template",
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#26C6DA"
            }
        },
        {
            "name": "TYPE",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#F9AE58"
            }
        },
        {
            "name": "TYPE BUILTIN",
            "scope": [
                "support.type.builtin",
                "storage.type.builtin",
                "storage.type.primitive"
            ],
            "settings": {
                "foreground": "#AB47BC",
                "fontStyle": "italic"
            }
        },
        {
            "name": "FUNCTION",
            "scope": [
                "entity.name.function",
                "support.function",
                "variable.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#42A5F5"
            }
        },
        {
            "name": "FUNCTION MACRO",
            "scope": [
                "entity.name.function.macro",
                "support.function.macro"
            ],
            "settings": {
                "foreground": "#26C6DA"
            }
        },
        {
            "name": "KEYWORD",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression",
                "storage.modifier",
                "storage.type.function"
            ],
            "settings": {
                "foreground": "#AB47BC"
            }
        },
        {
            "name": "OPERATOR",
            "scope": [
                "keyword.operator",
                "punctuation.accessor",
                "punctuation.separator"
            ],
            "settings": {
                "foreground": "#26C6DA"
            }
        },
        {
            "name": "PUNCTUATION",
            "scope": [
                "punctuation",
                "punctuation.definition.block",
                "punctuation.definition.parameters",
                "punctuation.section",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#26C6DA"
            }
        },
        {
            "name": "ATTRIBUTE",
            "scope": [
                "entity.other.attribute-name",
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "foreground": "#AB47BC"
            }
        },
        {
            "name": "TAG",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#EF5350"
            }
        },
        {
            "name": "NAMESPACE/MODULE",
            "scope": [
                "entity.name.namespace",
                "entity.name.module",
                "entity.name.package"
            ],
            "settings": {
                "foreground": "#F9AE58",
                "fontStyle": "italic"
            }
        },
        {
            "name": "STORAGE TYPE",
            "scope": [
                "storage.type",
                "storage.type.class",
                "storage.type.function"
            ],
            "settings": {
                "foreground": "#AB47BC"
            }
        },
        {
            "name": "SUPPORT CONSTANT",
            "scope": [
                "support.constant",
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#FF7043"
            }
        },
        {
            "name": "JSON KEY",
            "scope": [
                "support.type.property-name.json",
                "source.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#42A5F5"
            }
        },
        {
            "name": "MARKUP HEADING",
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#42A5F5",
                "fontStyle": "bold"
            }
        },
        {
            "name": "MARKUP BOLD",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#EF5350",
                "fontStyle": "bold"
            }
        },
        {
            "name": "MARKUP ITALIC",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#EF5350",
                "fontStyle": "italic"
            }
        },
        {
            "name": "MARKUP LINK",
            "scope": [
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#26C6DA",
                "fontStyle": "italic"
            }
        },
        {
            "name": "MARKUP CODE",
            "scope": [
                "markup.inline.raw",
                "markup.raw.block",
                "markup.fenced_code.block"
            ],
            "settings": {
                "foreground": "#7CB342"
            }
        },
        {
            "name": "DIFF ADDED",
            "scope": [
                "markup.inserted",
                "markup.inserted.diff"
            ],
            "settings": {
                "foreground": "#7CB342"
            }
        },
        {
            "name": "DIFF DELETED",
            "scope": [
                "markup.deleted",
                "markup.deleted.diff"
            ],
            "settings": {
                "foreground": "#EF5350"
            }
        },
        {
            "name": "INVALID",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#EF5350",
                "fontStyle": "bold"
            }
        }
    ]
}
